container_commands:
  01_syncdb:
    command: "python manage.py syncdb --noinput"
    leader_only: true
option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "HousingSolutionsAPI.settings"
    "PYTHONPATH": "./src"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: HousingSolutionsAPI.wsgi:application
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:environment:proxy:staticfiles":
    "/static/": "www/static/"

